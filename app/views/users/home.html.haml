- if @user.setup_state.blank?
  .row
    .col-md-6
      %h3 Current Elections
      = render partial: 'elections/table', locals: {elections: Election.all}
    .col-md-6
      %h3 Chapters
      = render partial: 'chapters/stats_table', locals: {chapters: Chapter.all}
- else
  = render partial: "#{@user.setup_state}"
